{
    "name": "ng2-material",
    "version": "0.0.0",
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "prestart": "npm run lint",
        "start": "ng serve",

        "prebuild": "npm run lint",
        "build": "ng build",
        "prebuild:prod": "npm run lint",
        "build:prod": "ng build --prod",

        "prebuild:gh-pages": "npm run lint",
        "build:gh-pages": "ng build --prod --base-href /ng2-material/",

        "preserve:prod": "npm run lint",
        "serve:prod": "ng serve --prod",

        "github-pages": "git subtree push --prefix dist origin gh-pages",
        "deploy": "npm-run-all build:gh-pages github-pages",

        "localtunnel": "lt --port 4200 --subdomain ahclibrary",
        "share": "npm-run-all --parallel serve:prod localtunnel",

        "lint": "tslint \"src/**/*.ts\"",
        "test": "ng test",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor",

        "jsf": "generate-json api/schema.json api/db.json",
        "api": "json-server --watch api/db.json"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "~2.1.0",
        "@angular/compiler": "~2.1.0",
        "@angular/core": "~2.1.0",
        "@angular/forms": "~2.1.0",
        "@angular/http": "~2.1.0",
        "@angular/material": "^2.0.0-alpha.10",
        "@angular/platform-browser": "~2.1.0",
        "@angular/platform-browser-dynamic": "~2.1.0",
        "@angular/router": "~3.1.0",
        "bootstrap": "^4.0.0-alpha.5",
        "core-js": "^2.4.1",
        "ng2-sidebar": "^1.7.0",
        "rxjs": "5.0.0-beta.12",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.6.23"
    },
    "devDependencies": {
        "@types/jasmine": "^2.2.30",
        "@types/node": "^6.0.42",
        "angular-cli": "1.0.0-beta.19-3",
        "codelyzer": "1.0.0-beta.1",
        "jasmine-core": "2.4.1",
        "jasmine-spec-reporter": "2.5.0",
        "karma": "1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.2.1",
        "protractor": "4.0.9",
        "ts-node": "1.2.1",
        "tslint": "3.13.0",
        "typescript": "~2.0.3",
        "webdriver-manager": "10.2.5",
        "localtunnel": "1.8.1",
        "npm-run-all": "3.1.1",
        "json-schema-faker": "0.3.6",
        "json-server": "0.8.22"
    }
}
